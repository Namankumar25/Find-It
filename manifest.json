{
    "manifest_version":2,
    "name":"Find It",
    "version":"1.0",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["content.js"]
        }
    ],
    "browser_action":{
        "default_icon":"files/icon.png",
        "default_popup":"files/index.html",
        "default_title":"Find a Word"
    }
    
}